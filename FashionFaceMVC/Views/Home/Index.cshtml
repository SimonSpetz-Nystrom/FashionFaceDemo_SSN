@model FashionFaceMVC.Models.LoginModel
@{
    ViewBag.Title = "FashionFace Demo";
}

<header>
    <h1>@ViewBag.Title</h1>
</header>

<div class="tracking">
    <b>Currently tracking:</b> <input type="text" value="#FashionFace" id="tracking-input" />
</div>
<div class="tweetsPerHour">
    <b>Tweets per hour: </b> 11
</div>
<div class="malesPerHour">
    <b>Males per hour: </b> 42
</div>
<div class="femalesPerHour">
    <b>Females per hour: </b> 3.14
</div>
@for (var i = 0; i < 10; i++)
{ <img src="https://www.getdigital.eu/web/getdigital/gfx/products/__generated__resized/1100x1100/Aufkleber_Trollface.jpg" class="twitter-image" /> }

@section scripts {
    <!--Script references. -->
    <!--The jQuery library is required and is referenced by default in _Layout.cshtml. -->
    <!--Reference the SignalR library. -->
    <script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="~/signalr/hubs"></script>
}
